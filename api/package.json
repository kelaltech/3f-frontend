{
  "name": "inventory-base-api",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "kelal tech. <info@kelaltech.com> (https://www.kelaltech.com)",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "run-p": "run-p",
    "run-s": "run-s",
    "prepublishOnly": "run-s prod",
    "clean": "del-cli dist *.tsbuildinfo",
    "build": "tsc",
    "lint": "prettier ./{*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,sass,json,md,html}",
    "format": "prettier --write ./{*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,sass,json,md,html}",
    "prod": "cross-env NODE_ENV=production run-s clean \"run-p format build test\"",
    "start": "run-s clean build \"run-p watch format dev\"",
    "dev": "nodemon --ext * -w dist -w ../../../core/account-api/dist -w ../../../core/inventory-api/dist",
    "watch": "tsc -w",
    "test": "jest --passWithNoTests",
    "test-live": "jest --passWithNoTests --watch",
    "UNSAFE_upgrade": "yarn add @types/koa-passport @types/nodemailer file:libraries/account-api-v0.0.3.tgz file:libraries/inventory-api-v0.0.2.tgz meseret meseret-utils"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prod & git update-index --again"
    }
  },
  "dependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/koa-passport": "^4.0.2",
    "@types/nodemailer": "^6.2.1",
    "@types/sharp": "^0.23.0",
    "account-api": "file:libraries/account-api-v0.0.3.tgz",
    "bcrypt": "^3.0.6",
    "meseret": "^1.9.2",
    "meseret-utils": "^0.0.6",
    "sharp": "^0.23.1"
  }
}
