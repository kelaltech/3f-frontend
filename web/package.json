{
  "name": "kaleab",
  "version": "0.1.0",
  "description": "Official Website of Kaleab S. Melkie. (https://bit.ly/kaleab)",
  "author": "Kaleab S. Melkie <kaleabmelkie@gmail.com> (https://bit.ly/kaleab)",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "start": "cross-env NODE_ENV=production next start",
    "build": "cross-env NODE_ENV=production next build",
    "export-only": "cross-env NODE_ENV=production next export",
    "export": "run-s build export-only",
    "analyze": "cross-env ANALYZE=true next build",
    "run-p": "run-p",
    "run-s": "run-s",
    "test": "jest --passWithNoTests --all",
    "test-live": "jest --passWithNoTests --all --watch",
    "lint": "prettier ./{*,public/**/*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,json,md,html}",
    "format": "prettier --write ./{*,public/**/*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,json,md,html}",
    "prod": "run-p format build test",
    "UNSAFE_upgrade": "yarn add @fortawesome/fontawesome-svg-core @fortawesome/free-brands-svg-icons @fortawesome/free-regular-svg-icons @fortawesome/free-solid-svg-icons @fortawesome/react-fontawesome @next/bundle-analyzer @types/aos @types/jest @types/node @types/qs @types/react @types/react-dom @zeit/next-css @zeit/next-sass animate.css aos cross-env husky isomorphic-unfetch jest lodash next next-offline node-sass npm-run-all prettier qs react react-dom react-parallax react-progressive-image typescript --latest"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-regular-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@next/bundle-analyzer": "^9.1.1",
    "@types/aos": "^3.0.2",
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "@types/qs": "^6.5.3",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.3",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-sass": "^1.0.1",
    "animate.css": "^3.7.2",
    "aos": "^2.3.4",
    "cross-env": "^6.0.3",
    "husky": "^3.0.9",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "next": "^9.1.1",
    "next-offline": "^4.0.6",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "qs": "^6.9.0",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-parallax": "^2.2.4",
    "react-progressive-image": "^0.6.0",
    "typescript": "^3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prod & git update-index --again"
    }
  }
}
