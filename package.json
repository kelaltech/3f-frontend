{
  "name": "3f-frontend",
  "version": "0.1.0",
  "description": "Official Website for 3F (Finfine Furniture Factory plc).",
  "author": "kelal tech plc <info@kelaltech.com> (https://www.kelaltech.com)",
  "private": true,
  "license": "UNLICENSED",
  "engines": {
    "node": "12.x",
    "yarn": "1.x"
  },
  "scripts": {
    "dev": "next dev",
    "start": "cross-env NODE_ENV=production next start",
    "build": "cross-env NODE_ENV=production next build",
    "export-only": "cross-env NODE_ENV=production next export",
    "export": "run-s build export-only",
    "analyze": "cross-env ANALYZE=true next build",
    "run-p": "run-p",
    "run-s": "run-s",
    "test": "jest --passWithNoTests --all",
    "test-live": "jest --passWithNoTests --all --watch",
    "lint": "prettier ./{*,public/**/*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,json,md,html}",
    "format": "prettier --write ./{*,public/**/*,src/**/*}.{ts,tsx,js,jsx,css,less,scss,json,md,html}",
    "prod": "run-p format build test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@mdx-js/loader": "^1.6.1",
    "@next/bundle-analyzer": "^9.3.6",
    "@next/mdx": "^9.3.6",
    "@types/aos": "^3.0.3",
    "@types/jest": "^25.2.1",
