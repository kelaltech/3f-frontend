    "@types/react-dom": "^16.9.7",
    "@types/react-share": "^3.0.3",
    "@types/react-slick": "^0.23.4",
    "@zeit/next-mdx": "^1.2.0",
    "aos": "^2.3.4",
    "axios": "^0.19.2",
    "brotli-webpack-plugin": "^1.1.0",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^7.0.2",
    "dotenv-load": "^2.0.0",
    "gerami": "^0.1.6",
    "husky": "^4.2.5",
    "isomorphic-unfetch": "^3.0.0",
    "jest": "^26.0.1",
    "next": "^9.3.6",
    "next-compose-plugins": "^2.2.0",
    "next-env": "^1.1.1",
    "next-fonts": "^1.1.0",
    "next-images": "^1.4.0",
