    "prettier": "^2.0.5",
    "qs": "^6.9.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-masonry-component": "^6.2.1",
    "react-parallax": "^3.0.3",
    "react-router-dom": "^5.1.2",
    "react-share": "^4.1.0",
    "react-slick": "^0.25.2",
    "slick-carousel": "^1.8.1",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s prod & git update-index --again"
    }
  }
